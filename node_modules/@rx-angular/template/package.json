{
  "name": "@rx-angular/template",
  "version": "18.0.0",
  "description": "**Fully** Reactive Component Template Rendering in Angular. @rx-angular/template aims to be a reflection of Angular's built in renderings just reactive.",
  "publishConfig": {
    "access": "public"
  },
  "author": "Michael Hladky <michael.hladky@push-based.io>",
  "homepage": "https://rx-angular.io/",
  "keywords": [
    "angular",
    "rxjs",
    "performance",
    "rendering",
    "template",
    "lazy templates",
    "memoized views",
    "scheduling",
    "coalescing",
    "concurrent rendering"
  ],
  "contributors": [
    {
      "name": "Michael Hladky",
      "email": "michael.hladky@push-based.io"
    },
    {
      "name": "Julian Jandl",
      "email": "julian@jandl.wien"
    },
    {
      "name": "Kirill Karnaukhov",
      "email": "karnaukhov.kh@gmail.com"
    },
    {
      "name": "Edouard Bozon",
      "email": "bozonedouard@gmail.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rx-angular/rx-angular.git"
  },
  "peerDependencies": {
    "@angular/core": "^18.0.0",
    "rxjs": "^6.5.3 || ^7.4.0",
    "@rx-angular/cdk": "^18.0.0"
  },
  "dependencies": {
    "tslib": "^2.4.1",
    "ng-morph": "^4.0.3"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "ng-update": {
    "migrations": "./schematics/migration.json"
  },
  "module": "fesm2022/template.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/template.mjs",
      "esm": "./esm2022/template.mjs",
      "default": "./fesm2022/template.mjs"
    },
    "./for": {
      "types": "./for/index.d.ts",
      "esm2022": "./esm2022/for/template-for.mjs",
      "esm": "./esm2022/for/template-for.mjs",
      "default": "./fesm2022/template-for.mjs"
    },
    "./if": {
      "types": "./if/index.d.ts",
      "esm2022": "./esm2022/if/template-if.mjs",
      "esm": "./esm2022/if/template-if.mjs",
      "default": "./fesm2022/template-if.mjs"
    },
    "./let": {
      "types": "./let/index.d.ts",
      "esm2022": "./esm2022/let/template-let.mjs",
      "esm": "./esm2022/let/template-let.mjs",
      "default": "./fesm2022/template-let.mjs"
    },
    "./push": {
      "types": "./push/index.d.ts",
      "esm2022": "./esm2022/push/template-push.mjs",
      "esm": "./esm2022/push/template-push.mjs",
      "default": "./fesm2022/template-push.mjs"
    },
    "./unpatch": {
      "types": "./unpatch/index.d.ts",
      "esm2022": "./esm2022/unpatch/template-unpatch.mjs",
      "esm": "./esm2022/unpatch/template-unpatch.mjs",
      "default": "./fesm2022/template-unpatch.mjs"
    },
    "./experimental/viewport-prio": {
      "types": "./experimental/viewport-prio/index.d.ts",
      "esm2022": "./esm2022/experimental/viewport-prio/template-experimental-viewport-prio.mjs",
      "esm": "./esm2022/experimental/viewport-prio/template-experimental-viewport-prio.mjs",
      "default": "./fesm2022/template-experimental-viewport-prio.mjs"
    },
    "./experimental/virtual-scrolling": {
      "types": "./experimental/virtual-scrolling/index.d.ts",
      "esm2022": "./esm2022/experimental/virtual-scrolling/template-experimental-virtual-scrolling.mjs",
      "esm": "./esm2022/experimental/virtual-scrolling/template-experimental-virtual-scrolling.mjs",
      "default": "./fesm2022/template-experimental-virtual-scrolling.mjs"
    }
  },
  "sideEffects": false
}