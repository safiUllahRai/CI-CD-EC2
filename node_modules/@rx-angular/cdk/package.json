    "rendering",
    "memoization",
    "zone flags",
    "change detection",
    "zone-less"
  ],
  "contributors": [
    {
      "name": "Michael Hladky",
      "email": "michael.hladky@push-based.io"
    },
    {
      "name": "Julian Jandl",
      "email": "julian@jandl.wien"
    },
    {
      "name": "Kirill Karnaukhov",
      "email": "karnaukhov.kh@gmail.com"
    },
    {
      "name": "Edouard Bozon",
