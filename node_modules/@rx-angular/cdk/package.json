    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/rx-angular-cdk.mjs",
      "esm": "./esm2022/rx-angular-cdk.mjs",
      "default": "./fesm2022/rx-angular-cdk.mjs"
